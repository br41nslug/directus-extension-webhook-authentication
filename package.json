{
	"name": "directus-extension-webhook-authentication",
	"description": "Directus Flows Operation to check webhook authorization",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "operation",
				"name": "check-auth",
				"source": {
					"app": "src/check-auth/app.js",
					"api": "src/check-auth/api.js"
				}
			},
			{
				"type": "operation",
				"name": "check-perms",
				"source": {
					"app": "src/check-perms/app.js",
					"api": "src/check-perms/api.js"
				}
			}
		],
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "12.0.2",
		"vue": "^3.5.8"
	}
}
